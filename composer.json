{
    "name":         "tadcka/routing",
    "homepage":     "https://github.com/tadcka/Routing",
    "description":  "Tadcka routing component.",
    "keywords":     ["routing"],
    "type":         "library",
    "license":      "MIT",
    "authors": [
        {
            "name":   "Tadas Gliaubicas",
            "email":  "tadcka89@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4",
        "symfony-cmf/routing": "^1.2 || ^2.2",
        "aferrandini/urlizer": "1.0.*"
    },
    "require-dev": {
